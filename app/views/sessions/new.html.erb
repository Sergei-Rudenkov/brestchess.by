<h1>Авторизация для админа</h1>
<%= @putan %>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Вход", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
